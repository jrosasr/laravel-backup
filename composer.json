{
    "name": "jrosasr/laravel-backup",
    "description": "Realiza copias de seguridad de la base de datos de tu aplicaci√≥n Laravel de una forma simple y eficiente.",
    "keywords": ["backup", "laravel"],
    "type": "library",
    "license": "MIT",
    "version": "0.1.0",
    "homepage": "https://github.com/jrosasr/laravel-backup",
    "authors": [
        {
            "name": "Juan Rosas",
            "email": "juan.rosasr01@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0 || ^11.0",
        "symfony/process": "^6.0 || ^7.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jrosasr\\LaravelBackup\\": "./src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jrosasr\\LaravelBackup\\BackupServiceProvider"
            ]
        }
    }
}